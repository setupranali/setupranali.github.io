{
  "name": "grafana-setupranali-datasource",
  "version": "1.0.0",
  "description": "SetuPranali data source plugin for Grafana",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --passWithNoTests",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx ./src",
    "typecheck": "tsc --noEmit",
    "server": "docker-compose up --build"
  },
  "author": "SetuPranali Community",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/setupranali/setupranali.github.io.git"
  },
  "devDependencies": {
    "@grafana/eslint-config": "^6.0.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@swc/core": "^1.3.90",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.50.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "style-loader": "^3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "dependencies": {
    "@emotion/css": "^11.11.0",
    "@grafana/data": "^10.0.0",
    "@grafana/runtime": "^10.0.0",
    "@grafana/ui": "^10.0.0",
    "lodash": "^4.17.21",
