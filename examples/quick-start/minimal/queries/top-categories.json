{
  "dataset": "orders",
  "dimensions": ["category"],
  "metrics": ["total_revenue", "order_count"],
  "filters": [
    {
      "dimension": "status",
      "operator": "=",
      "value": "delivered"
    }
  ],
  "orderBy": [
    {
      "field": "total_revenue",
      "direction": "desc"
    }
  ],
  "limit": 10
}

